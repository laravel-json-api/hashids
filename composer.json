{
    "name": "laravel-json-api/hashids",
    "description": "Encode model IDs to JSON:API resource IDs.",
    "keywords": [
        "jsonapi.org",
        "json-api",
        "jsonapi",
        "laravel"
    ],
    "homepage": "https://github.com/laravel-json-api/hashids",
    "support": {
        "issues": "https://github.com/laravel-json-api/hashids/issues"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Cloud Creativity Ltd",
            "email": "info@cloudcreativity.co.uk"
        },
        {
            "name": "Christopher Gammie",
            "email": "contact@gammie.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-json-api/core": "^3.0",
        "laravel-json-api/eloquent": "^3.0",
        "vinkla/hashids": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "LaravelJsonApi\\HashIds\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelJsonApi\\HashIds\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
